
(define method (fact n)
  with {n Integer}
  (if (< n 1)
      1
      (* n (- n 1))))

(define method (fact2 n)
  with {n Integer}
  (reduce * (take n (range-from 1))
          :initial-value 1))

(define constant N (limited Integer :minimum 1 :maximum nothing))

(define method (fact n)
  with {n (exactly 1)}
  (the <fixnum> 1))

(define method (fact n)
  with {n N}
  (* n (- n 1)))






