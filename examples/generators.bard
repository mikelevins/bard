;;; all the natural numbers
(define variable $ints
  (generate ((x 1))
    (yield x)
    (resume (+ x 1))))

(take 5 $ints)

;;; fibonacci sequence
(define variable $fibs
  (generate ((x 1)
             (y 1))
    (yield x)
    (resume y (+ x y))))

;;;(take 12 $fibs)

(def $iter (iterate rest [0 1 2 3 4 5]))

